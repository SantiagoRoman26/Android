<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.santiagoroman">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SantiagoRoman">
        <activity android:name="com.example.santiagoroman.ActivityVolleyAlumno"></activity>
        <activity android:name="com.example.santiagoroman.ActivityBDocentes" />
        <activity android:name="com.example.santiagoroman.ActivityMemoriaExterna" />
        <activity android:name="com.example.santiagoroman.ActivityMemoriaPrograma" />
        <activity android:name="com.example.santiagoroman.ActivityMemoriaInterna" />
        <activity
            android:name="com.example.santiagoroman.PrincipalActivity"
            android:label="@string/title_activity_principal"
            android:theme="@style/Theme.SantiagoRoman.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.santiagoroman.NombresActivity"
            android:label="@string/title_activity_nombres"
            android:theme="@style/Theme.SantiagoRoman.NoActionBar" />
        <activity
            android:name="com.example.santiagoroman.FragmentosActivity"
            android:label="@string/title_activity_fragmentos"
            android:theme="@style/Theme.SantiagoRoman.NoActionBar" />
        <activity android:name="com.example.santiagoroman.LoginActivity" />
        <activity android:name="com.example.santiagoroman.MainActivity" />
    </application>

</manifest>